enum NeedStrategy    //分配策略
{
	RANDOM_DOCTOR = 1,
	RANDOM_DRUGGIST=7,
	ASSIGN_DOCTOR=3,
	ASSIGN_DRUGGIST,
	RANDOM_CP,
	RANDOM_WP,
	ASSIGN_DRUGGIST_EX,
};

enum MatchFailure    //匹配错误码
{
	NO_DRUGGIST = 1,
	NO_DOCTOR,
	NO_ASSIGN_DRUGGIST,
	NO_ASSIGN_DOCTOR,
	NO_PROXY_SERVER,
	UNKNOW_ERROR,
};

enum PharmacistType  //医药师类型
{
	CHINESE_PHARMACIST,
	WESTERN_PHARMACIST,
	PRACTICE_CP,
	PRACTICE_WP,
	医生DOCTOR=4,
	保险师insurance=5,
};